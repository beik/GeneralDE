<?xml version="1.0" encoding="GBK" standalone="yes"?>
<metalib tagsetversion="1" name="" version="1">
    <include name="svr_center_data" file="svr_center_data.xml"/>

    <!-- 协议定义 -->

    <!-- 注册或者握手协议 -->
    <struct name="svr_center_req_join" version="1">
        <entry name="id" type="svr_center_svr_id" id="1"/>
    </struct>

    <!-- 查询指定类型服务器列表 -->
    <struct name="svr_center_req_query_svr_by_type" version="1">
        <entry name="svr_type" type="uint16" id="1"/>
    </struct>

    <!-- 整体协议定义 -->
    <macrosgroup name="SVR_CENTER_CMD_ID">
        <macro name="SVR_CENTER_CMD_REQ_JOIN" value="1"/>
        <macro name="SVR_CENTER_CMD_RES_JOIN" value="2"/>

        <macro name="SVR_CENTER_CMD_REQ_QUERY_SVR_BY_TYPE" value="3"/>
        <macro name="SVR_CENTER_CMD_RES_QUERY_SVR_BY_TYPE" value="4"/>

    </macrosgroup>

    <union name="svr_center_pkg_data" version="1" desc="">
        <entry name="svr_center_req_join" type="svr_center_req_join" id="SVR_CENTER_CMD_REQ_JOIN"/>

        <entry name="svr_center_req_query_svr_by_type" type="svr_center_req_query_svr_by_type" id="SVR_CENTER_CMD_REQ_QUERY_SVR_BY_TYPE"/>
    </union>

</metalib>
